{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "linebreak-style": "off",
    "quotes": "off",
    "indent": "off",
    "comma-spacing": "off",
    "keyword-spacing": "off",
    "space-before-blocks":  "off",
    "require-jsdoc": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}